{
  "name": "starter-saas",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "convex": "^1.27.0",
      "better-auth": "1.3.27",
      "@convex-dev/better-auth": "^0.9.1",
      "@types/node": "^20",
      "typescript": "^5",
      "eslint": "^9.39.1",
      "@typescript-eslint/parser": "^8.48.1"
    }
  },
  "scripts": {
    "check": "turbo run check:biome //#check:eslint check:eslint check:types check:spell //#check:sherif //#check:knip --continue",
    "check:biome": "biome check .",
    "check:eslint": "eslint --concurrency auto .",
    "check:spell": "bunx cspell --config ./cspell.json --unique --show-context --show-suggestions \".\"",
    "check:sherif": "bunx sherif",
    "check:knip": "knip",
    "fix": "biome check --write .",
    "prepare": "husky",
    "ruler:apply": "bunx @intellectronica/ruler@latest apply --local-only",
    "dev": "dotenv -- dotenv -- turbo dev",
    "build": "turbo build",
    "check:types": "turbo check:types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @starter-saas/backend dev",
    "dev:setup": "turbo -F @starter-saas/backend dev:setup",
    "dev:storybook": "turbo -F @starter-saas/storybook dev",
    "dev:emails": "turbo -F @starter-saas/emails dev",
    "test": "turbo test",
    "test:watch": "vitest --watch",
    "test:e2e": "turbo -F @starter-saas/e2e test:e2e",
    "test:e2e:failed": "turbo -F @starter-saas/e2e test:e2e:failed",
    "test:e2e:shared": "turbo -F @starter-saas/e2e test:e2e:shared",
    "test:e2e:ui": "turbo -F @starter-saas/e2e test:ui",
    "test:e2e:storybook": "turbo -F @starter-saas/e2e test:storybook",
    "coverage:unit": "vitest run --coverage",
    "coverage:e2e": "turbo -F @starter-saas/e2e test:e2e:coverage",
    "coverage": "bun run coverage:unit && bun run coverage:e2e",
    "coverage:report": "bunx rimraf .nyc_output coverage-merged && mkdir -p .nyc_output && cp coverage/coverage-final.json .nyc_output/vitest.json 2>/dev/null || true && cp apps/e2e/coverage-report/coverage-istanbul.json .nyc_output/e2e.json 2>/dev/null || true && bun run scripts/coverage-all-files.ts && bunx nyc report && open coverage-merged/index.html",
    "lint:skills": "bun run scripts/lint-claude-skills.ts",
    "lint:steps": "turbo -F @starter-saas/e2e lint:steps",
    "intl:lint": "turbo intl:lint",
    "intl:extract": "turbo intl:extract",
    "chrome": "open -na 'Google Chrome' --args --user-data-dir=\"$(pwd)/.chrome-profile\"",
    "knip": "knip",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "eslint-plugin-better-tailwindcss": "^1.4.1",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cspell/dict-fr-fr": "^2.3.2",
    "@cucumber/cucumber": "^12.2.0",
    "@cucumber/gherkin": "^37.0.0",
    "@lingui/babel-plugin-lingui-macro": "^5.6.1",
    "@lingui/loader": "^5.6.1",
    "@playwright/test": "^1.57.0",
    "@starter-saas/eslint-config": "workspace:*",
    "@starter-saas/vitest-config": "workspace:*",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@typescript/native-preview": "7.0.0-dev.20251211.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "baseline-browser-mapping": "^2.9.6",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "framer-motion": "^12.23.25",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.2.0",
    "knip": "^5.71.0",
    "lint-staged": "^16.1.2",
    "nyc": "^17.1.0",
    "playwright-bdd": "^8.4.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.1.2",
    "sharp-cli": "^5.2.0",
    "turbo": "^2.5.4",
    "ultracite": "6.3.4",
    "vitest": "^4.0.14"
  },
  "packageManager": "bun@1.3.2",
  "engines": {
    "node": ">=18 <25"
  },
  "patchedDependencies": {},
  "dependencies": {
    "@convex-dev/polar": "^0.7.3"
  }
}
