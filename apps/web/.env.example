# Copy in .env.local and fill in the values

# Development port (for running multiple worktrees)
# Worktree 1 (main): PORT=3001
# Worktree 2: PORT=3002
# Worktree 3: PORT=3003
PORT=3001

# Convex
NEXT_PUBLIC_CONVEX_URL=
NEXT_PUBLIC_CONVEX_SITE_URL=

# Sentry Error Monitoring
# Get your DSN from: https://sentry.io/settings/{org}/projects/{project}/keys/
NEXT_PUBLIC_SENTRY_DSN=

# Sentry Build Configuration (for source map uploads)
# Only needed in CI/production builds
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=
